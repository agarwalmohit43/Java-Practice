package after;
class Cycle{
	int weight=500;
	public void display(){
		System.out.println(weight);
	}
}

class ByCylce extends Cycle{
	int height=500;
	public void display(){
		System.out.println(weight);
		System.out.println(height);
	}
}
public class Iinheritance {

	public static void main(String[] args) {

		ByCylce obj=new ByCylce();
		
	}

}
