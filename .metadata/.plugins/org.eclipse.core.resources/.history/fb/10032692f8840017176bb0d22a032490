package after;

public class ArrayExample {
	static int[] arr=new int[15];
	
	public static void main(String[] args) {
		initialise()
		for(int i:arr){
			System.out.print(i+" ");
		}
		insertAtPos(15,5);
		
	}
	private static void initialise() {
		for(int i=0;i<10;i++){
			arr[i]=i;
		}
	}
	private static void insertAtPos(int value, int pos) {

		for(int i=0;i<arr.length;i++){
			
		}
		
	}

}
