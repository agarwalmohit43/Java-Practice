package thread2;

class ThreadA implements Runnable{

	int total;
	@Override
	public void run() {

		for(int i=0;i<10;i++){
			total+=i;
		}
		notify();
	}
	
}

public class ThreadB {

	public static void main(String[] args){
		ThreadA obj=new ThreadA();
		Thread t1=new Thread(obj);
		t1.start();
		
	}
}
